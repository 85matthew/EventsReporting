<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>


<table>
  <thead>
    <tr>
      <th>App name</th>
      <th>User</th>
      <th>User type</th>
      <th>Tenant</th>
      <th>Url</th>
      <th>Domain</th>
      <th>Duration</th>
      <th>Search words</th>
      <th>Start time stamp</th>
      <th>End time stamp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.app_name %></td>
        <td><%= event.user_id %></td>
        <td><%= event.user_type %></td>
        <td><%= event.tenant %></td>
        <td><%= event.url %></td>
        <td><%= event.domain %></td>
        <td><%= event.duration %></td>
        <td><%= event.search_words %></td>
        <td><%= event.start_time_stamp %></td>
        <td><%= event.end_time_stamp %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>

<%= form_for @event do |f| %>
	<%f.collection_select :event_id, domains, :id, :name%>
<% end %>
